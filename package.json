{
  "name": "codemap",
  "displayName": "codemap",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.61.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:codemap.helloWorld"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "codemap.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "codemap.add",
        "title": "Add to codemap"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "editorHasSelection",
          "command": "codemap.add",
          "group": "navigation",
          "title": "Add to codemap"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "lint": "eslint src --ext ts",
    "test": "jest"
  },
  "devDependencies": {
    "@types/ajv": "^1.0.0",
    "@types/glob": "^7.1.4",
    "@types/jest": "^27.4.0",
    "@types/node": "14.x",
    "@types/object-hash": "^2.2.1",
    "@types/react": "^17.0.34",
    "@types/react-dom": "^17.0.11",
    "@types/styled-components": "^5.1.15",
    "@types/uuid": "^8.3.1",
    "@types/vscode": "^1.61.0",
    "@types/vscode-webview": "^1.57.0",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "@vscode/test-electron": "^1.6.2",
    "css-loader": "^6.5.1",
    "eslint": "^8.1.0",
    "glob": "^7.1.7",
    "jest": "^27.4.5",
    "process": "^0.11.10",
    "style-loader": "^3.3.1",
    "ts-jest": "^27.1.2",
    "ts-loader": "^9.2.5",
    "typescript": "^4.4.4",
    "webpack": "^5.52.1",
    "webpack-cli": "^4.8.0"
  },
  "dependencies": {
    "ajv": "^8.7.1",
    "ajv-formats": "^2.1.1",
    "elkjs": "^0.7.1",
    "node-cache": "^5.1.2",
    "object-hash": "^2.2.0",
    "re-resizable": "^6.9.1",
    "react": "^17.0.2",
    "react-contexify": "^5.0.0",
    "react-dom": "^17.0.2",
    "react-flow-renderer": "^10.0.0-next.30",
    "styled-components": "^5.3.3",
    "use-reducer-async": "^2.1.0",
    "uuid": "^8.3.2"
  },
  "resolutions": {
    "styled-components": "^5"
  }
}
